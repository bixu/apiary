{
  "calculations": [
    {"op": "COUNT", "alias": "total_requests"},
    {"op": "AVG", "column": "duration_ms", "alias": "avg_duration"},
    {"op": "P95", "column": "duration_ms", "alias": "p95_duration"}
  ],
  "filters": [
    {"column": "service", "op": "=", "value": "web"},
    {"column": "status_code", "op": "<", "value": 400}
  ],
  "breakdowns": ["endpoint", "region"],
  "orders": [
    {"op": "avg_duration", "order": "desc"}
  ],
  "time_range": 3600,
  "granularity": 60,
  "limit": 100
}